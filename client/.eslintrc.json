{
    "extends": ["plugin:prettier/recommended", "plugin:vue/recommended", "prettier/vue", "eslint:recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"]
    },
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "env": {
        "node": true,
        "browser": true
    }
}